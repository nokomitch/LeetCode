class Solution:
    def countPrimes(self, n: int) -> int:
        primes = [True for i in range(n)]
        i = 2
        while i*i < n:
            if primes[i] == True:
                for j in range(i*i, n, i):
                    primes[j] = False
            i += 1
        
        count = 0
        for i in range(2, n):
            if primes[i] == True:
                count += 1
        return count
    

